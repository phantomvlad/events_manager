<%= form_for [@event, photo], data: { turbo: false },
             html: { multipart: true } do |f| %>
  <%= render partial: 'partials/form_errors', locals: { model: photo } %>

  <div class="mb-3 col-6">
    <%= f.label :image %>
    <%= f.file_field :image, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= f.submit t('buttons.send'), class: 'btn btn-primary' %>
  </div>
<% end %>
